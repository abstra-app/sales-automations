{
  "manifest_version": 3,
  "name": "Abstra Linkedin Scraper",
  "description": "Send all linkedin data to CRM",
  "version": "1.0",
  "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "abstra_logo.svg"
  },
  "content_scripts": [{
    "matches": ["*://*.linkedin.com/*"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }]
}
